{
  "name": "oh-vue-icon-v3",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:lib-css",
    "build:lib": "vue-cli-service build --target lib --name icon ./src/index.js",
    "build:lib-css": "EXTRACT_CSS=true npm run build:lib",
    "build:demo": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "prettier": "^2.2.1"
  }
}
