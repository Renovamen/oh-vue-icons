{
  "name": "oh-vue-icons",
  "version": "0.1.11",
  "description": "Include inline SVG icons from different popular icon packs in Vue easily.",
  "keywords": [
    "font awesome",
    "remix icon",
    "bootstrap icons",
    "line awesome",
    "octions",
    "simple icons",
    "academicons",
    "weather icons",
    "pixel icons",
    "flat color icons",
    "vscode icons",
    "game icons",
    "cryptocurrency Icons",
    "pokemon icons",
    "flag icon",
    "vue",
    "icons",
    "iconpacks"
  ],
  "homepage": "https://oh-vue-icons.vercel.app",
  "bugs": {
    "url": "https://github.com/Renovamen/oh-vue-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Renovamen/oh-vue-icons"
  },
  "license": "MIT",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "scripts": {
    "build": "node scripts/build.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "nuxt",
    "docs:build": "nuxt generate",
    "docs:preview": "cd docs/dist && http-server",
    "download": "sh ./scripts/download.sh",
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.vue ./",
    "pub": "sh ./scripts/publish.sh",
    "start": "nuxt start",
    "submodule": "git submodule update --init",
    "update": "sh ./scripts/update.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@nuxt/content": "^1.13.1",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "glob-promise": "^4.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nuxt": "^2.15.2",
    "nuxt-i18n": "^6.20.3",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.49.0",
    "svgo": "^1.3.2",
    "vue-slider-component": "^3.2.11",
    "vue-swatches": "^2.1.0"
  }
}
